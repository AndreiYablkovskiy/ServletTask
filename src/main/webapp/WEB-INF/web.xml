<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>homeServlet</servlet-name>
    <servlet-class>controller.servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>NecklaceServlet</servlet-name>
    <servlet-class>controller.servlet.NecklaceServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DescriptionServlet</servlet-name>
    <servlet-class>controller.servlet.DescriptionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SortServlet</servlet-name>
    <servlet-class>controller.servlet.SortServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>homeServlet</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NecklaceServlet</servlet-name>
    <url-pattern>/necklace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DescriptionServlet</servlet-name>
    <url-pattern>/description</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SortServlet</servlet-name>
    <url-pattern>/sort</url-pattern>
  </servlet-mapping>
</web-app>
